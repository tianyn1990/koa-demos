{
  "name": "koa-demos",
  "version": "1.0.2",
  "description": "a collection of simple demos of Koa",
  "scripts": {
    "build": "docker image build -t tianyn1990/koa-demos:latest ./",

    "runbash": "docker container run --rm -p 8000:3000 -it tianyn1990/koa-demos:latest /bin/bash",
    "runCMD": "docker container run --rm -p 8000:3000 -it tianyn1990/koa-demos:latest",

    "docker:stop": "docker container stop ",
    "docker:kill": "docker container kill ",

    "publish": "docker image push tianyn1990/koa-demos:latest"
  },
  "keywords": ["Koa", "Node"],
  "author": "Ruan Yifeng",
  "license": "ISC",
  "dependencies": {
    "fs.promised": "^3.0.0",
    "koa": "^2.3.0",
    "koa-body": "^2.3.0",
    "koa-compose": "^4.0.0",
    "koa-route": "^3.2.0",
    "koa-static": "^4.0.1"
  }
}
